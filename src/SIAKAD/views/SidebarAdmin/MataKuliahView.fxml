<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>

<TableView fx:id="mataKuliahTableView" prefHeight="620.0" prefWidth="248.0"
    xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="SIAKAD.controllers.SidebarAdmin.MataKuliahController">
    <columns>
        <TableColumn text="Kode" fx:id="kodeColumn" prefWidth="100" style="-fx-alignment: CENTER;" />
        <TableColumn text="Nama Mata Kuliah" fx:id="namaColumn" prefWidth="200"
            style="-fx-alignment: CENTER;" />
        <TableColumn text="Prodi" fx:id="prodiColumn" prefWidth="150" style="-fx-alignment: CENTER;" />
        <TableColumn text="SKS" fx:id="sksColumn" prefWidth="150" style="-fx-alignment: CENTER;" />
        <TableColumn text="Dosen Pengajar" fx:id="dosenPengajarColumn" prefWidth="150"
            style="-fx-alignment: CENTER;" />
    </columns>
</TableView>

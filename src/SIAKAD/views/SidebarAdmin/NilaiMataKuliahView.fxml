   fx:controller="SIAKAD.controllers.SidebarAdmin.NilaiMataKuliahController">
   <columns>
      <TableColumn fx:id="kodeColumn" prefWidth="100" style="-fx-alignment: CENTER;"
         text="Kode Mata Kuliah" />
      <TableColumn fx:id="namaMataKuliahColumn" prefWidth="150" style="-fx-alignment: CENTER;"
         text="Nama Mata Kuliah" />
      <TableColumn fx:id="namaMahasiswaColumn" prefWidth="150" style="-fx-alignment: CENTER;"
         text="Nama Mata Kuliah" />
      <TableColumn fx:id="nilaiColumn" prefWidth="150" style="-fx-alignment: CENTER;" text="Nilai" />
   </columns>
   <contextMenu>
      <ContextMenu fx:id="contextMenu">
         <items>
            <MenuItem fx:id="editNilaiMenu" mnemonicParsing="false" text="Edit nilai" />
         </items>
      </ContextMenu>
   </contextMenu>

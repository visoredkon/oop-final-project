<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>

<TableView fx:id="dosenTableView" prefHeight="620.0" prefWidth="248.0"
    xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="SIAKAD.controllers.SidebarMahasiswa.DosenController">
    <columns>
        <TableColumn text="NIP" fx:id="nipColumn" prefWidth="100" style="-fx-alignment: CENTER;" />
        <TableColumn text="Nama" fx:id="namaColumn" prefWidth="200" style="-fx-alignment: CENTER;" />
        <TableColumn text="Prodi" fx:id="prodiColumn" prefWidth="150" style="-fx-alignment: CENTER;" />
        <TableColumn text="Jenis Kelamin" fx:id="jenisKelaminColumn" prefWidth="150"
            style="-fx-alignment: CENTER;" />
        <TableColumn text="Nomor Telepon" fx:id="nomorTeleponColumn" prefWidth="150"
            style="-fx-alignment: CENTER;" />
        <TableColumn text="Alamat" fx:id="alamatColumn" prefWidth="250"
            style="-fx-alignment: CENTER;" />
    </columns>
</TableView>

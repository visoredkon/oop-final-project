<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<VBox xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="SIAKAD.controllers.SidebarMahasiswa.NilaiMataKuliahController">
   <children>
      <Text fx:id="ipkText" strokeType="OUTSIDE" strokeWidth="0.0" text="IPK: ">
         <font>
            <Font size="22.0" />
         </font>
      </Text>
      <TableView fx:id="nilaiMataKuliahTableView" prefHeight="620.0" prefWidth="248.0">
         <columns>
            <TableColumn fx:id="kodeColumn" prefWidth="100" style="-fx-alignment: CENTER;"
               text="Kode Mata Kuliah" />
            <TableColumn fx:id="namaMataKuliahColumn" prefWidth="150" style="-fx-alignment: CENTER;"
               text="Nama Mata Kuliah" />
            <TableColumn fx:id="nilaiColumn" prefWidth="150" style="-fx-alignment: CENTER;"
               text="Nilai" />
         </columns>
      </TableView>
   </children>
</VBox>
